{
  "name": "@pi-lib/charts",
  "version": "0.2.0",
  "description": "Simple charting solution driven by d3 and ES.Next",
  "main": "./dist/pi-lib-charts.js",
  "repository": "https://github.com/lancerael/pi-charts",
  "author": "Lance Taylor",
  "license": "MIT",
  "private": false,
  "type": "module",
  "keywords": [
    "charts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prettier": "prettier -w ./src/**/*.ts",
    "eslint": "eslint --ext .ts",
    "lint": "pnpm prettier && pnpm eslint",
    "test": "nyc pnpm test:quick",
    "test:quick": "jasmine-ts --config=jasmine.json",
    "test:coverage": "nyc --reporter=lcov --reporter=text-summary pnpm test:quick",
    "test:full": "pnpm lint && yarn test:coverage"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/d3-array": "^2.9.0",
    "@types/d3-axis": "^2.0.0",
    "@types/d3-color": "^2.0.1",
    "@types/d3-ease": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-transition": "^2.0.0",
    "@types/jasmine": "^3.6.3",
    "@types/jsdom": "^16.2.6",
    "@types/node": "^18.15.1",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.0",
    "jsdom": "^16.4.0",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "resize-observer-polyfill": "^1.5.1",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "dependencies": {
    "d3-array": "^2.11.0",
    "d3-axis": "^2.0.0",
    "d3-color": "^2.0.0",
    "d3-ease": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-selection": "^2.0.0",
    "d3-transition": "^2.0.0"
  }
}
